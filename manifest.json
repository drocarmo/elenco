{
  "manifest_version": 2,

  "name": "Elenco",
  "description":"Manage your tasks",
  "version":"0.1",
  "permissions": [
    "storage"
  ],
 "icons": {
    "16":"/assets/images/icon-16.png",
    "48":"/assets/images/icon-48.png",
    "128":"/assets/images/icon-128.png"
  },
  "browser_action": {
  	"default_title":"Elenco",
    "default_icon":"/assets/images/icon.png"
  },
  "chrome_url_overrides": {
  	"newtab":"index.html"
  }
}
