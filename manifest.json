{
  "manifest_version": 2,

  "name": "Elenco",
  "description":"Manage your tasks",
  "version":"0.1",
  "permissions": [
    "storage"
  ],
  "browser_action": {
  	"default_title":"Elenco",
    "default_icon":"assets/images/icon.png"
  },
  "chrome_url_overrides": {
  	"newtab":"index.html"
  }
}
